{
  "name": "@dylanjs/utils",
  "version": "0.0.2",
  "description": "项目中常用的工具方法的工具库",
  "author": "Dylan <my466879168@gmail.com> (https://github.com/mengyang94982/)",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "lint": "eslint . --fix",
    "format": "dy prettier-write",
    "commit": "dy git-commit",
    "cleanup": "dy cleanup",
    "ncu": "dy ncu",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "build-pkg": "pnpm -r --filter='./packages/*' run build",
    "build": "pnpm typecheck && unbuild && pnpm build-pkg",
    "publish-pkg": "pnpm publish --access public --no-git-checks",
    "release": "dy release && pnpm build && pnpm publish-pkg"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@dylanjs/cli": "workspace:*"
  }
}

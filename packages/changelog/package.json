{
  "name": "@dylanjs/changelog",
  "version": "0.0.1",
  "description": "generate changelog form git tags and commits for github",
  "author": "Dylan <my466879168@gmail.com> (https://github.com/mengyang94982/)",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "lint": "eslint . --fix",
    "format": "dy prettier-write",
    "commit": "dy git-commit",
    "cleanup": "dy cleanup",
    "ncu": "dy ncu",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "build-pkg": "pnpm -r --filter='./packages/*' run build",
    "build": "pnpm typecheck && unbuild && pnpm build-pkg",
    "publish-pkg": "pnpm  publish --access public --no-git-checks",
    "release": "dy release && pnpm build && pnpm publish-pkg"
  },
  "dependencies": {
    "@dylanjs/cli": "workspace:*",
    "cli-progress": "^3.12.0",
    "convert-gitmoji": "0.1.3",
    "ofetch": "^1.3.3"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0"
  }
}

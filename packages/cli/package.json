{
  "name": "@dylanjs/cli",
  "version": "0.0.6",
  "description": "DylanJS's command line tools",
  "author": "Dylan <my466879168@gmail.com> (https://github.com/mengyang94982/)",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "dylan": "dist/index.cjs",
    "dy": "dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "dev":"unbuild",
    "test":"pnpm publish --no-git-checks",
    "lint": "eslint . --fix",
    "format": "dy prettier-write",
    "commit": "dy git-commit",
    "cleanup": "dy cleanup",
    "ncu": "dy ncu",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "build-pkg": "pnpm -r --filter='./packages/*' run build",
    "build": "pnpm typecheck && unbuild && pnpm build-pkg",
    "publish-pkg": "pnpm publish --access public --no-git-checks",
    "release": "dy release && pnpm build && pnpm publish-pkg"
  },
  "keywords": [],
  "dependencies": {
    "@dylanjs/cli": "link:",
    "c12": "^1.4.2",
    "cac": "^6.7.14",
    "lint-staged": "^14.0.1",
    "npm-check-updates": "^16.13.2",
    "rimraf": "^5.0.1"
  },
  "devDependencies":{
    "@dylanjs/changelog": "workspace:*"
  }
}

{
  "name": "@dylanjs/cli",
  "version": "0.0.11",
  "description": "DylanJS's command line tools",
  "author": "Dylan <my466879168@gmail.com> (https://github.com/mengyang94982/)",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "dylan": "dist/index.cjs",
    "dy": "dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "ubnuild": "unbuild",
    "pub": "pnpm publish --no-git-checks",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "commit": "dy git-commit",
    "build": "pnpm typecheck && unbuild"
  },
  "dependencies": {
    "@dylanjs/cli": "link:",
    "c12": "^1.4.2",
    "cac": "^6.7.14",
    "enquirer": "^2.4.1",
    "kolorist": "^1.8.0",
    "rimraf": "^5.0.1",
    "lint-staged": "^14.0.1"
  }
}
